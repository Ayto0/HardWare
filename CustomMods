    local old
    local DamageIndicatorCustom = {["Enabled"] = false}
    DamageIndicatorCustom = GuiLibrary["ObjectsThatCanBeSaved"]["RenderWindow"]["Api"].CreateOptionsButton({
        ["Name"] = "Damage Indicatiors",
        ["HoverText"] = "Customizable (credits to meteor devs )"
        ["Function"] = function(Callback)
                if Callback then
                    old = debug.getupvalue(bedwars["DamageIndicator"],10,{Create})
                    debug.setupvalue(bedwars["DamageIndicator"],10,{
                        Create = function(self,obj,...)
                            spawn(function()
                                pcall(function()
                                    obj.Parent.Text = Messages[math.random(1,#IndicatorTexts.ObjectList) or "Ting!"]
                                    obj.Parent.TextColor3 =  Color3.fromHSV(tick()%5/5,1,1)
                                end)
                            end)
                            return game:GetService("TweenService"):Create(obj,...)
                        end
                    })
                else
                    debug.setupvalue(bedwars["DamageIndicator"],10,{
                        Create = old
                    })
                    old = nil
                end
            end
        end
    })
    local IndicatorTexts = DamageIndicatorCustom.CreateTextList({
        Name = "DamageIndicatorText",
        TempText = "phrase",
    })
end)
